{
  "name": "dynamorm",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.12.1"
  },
  "types": "bundle/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-test-env": "tsc --project ./tests/env/tsconfig.json",
    "build-tests": "tsc --project ./tests/tsconfig.json",
    "build-legacy-tests": "tsc --project ./tests/legacy/tsconfig.json",
    "build-babel-tests": "cd ./tests/babel && babel . --out-dir . --out-file-extension .min.js",
    "prebundle": "tsc --project ./tsconfig.declarations.json",
    "bundle": "esbuild src/index.ts src/operators.ts --bundle --packages=external --platform=node --target=node16 --outdir=bundle",
    "clean": "rm -r bundle && rm -r lib",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@asn.aeb/async-array": "^1.5.9",
    "@aws-sdk/client-dynamodb": "^3.266.0",
    "@aws-sdk/lib-dynamodb": "^3.266.0",
    "amazon-dax-client": "^1.2.9",
    "aws-sdk": "^2.1310.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@types/amazon-dax-client": "^1.2.5",
    "@types/node": "^18.13.0",
    "esbuild": "^0.17.6",
    "typescript": "^5.0.0-dev.20230207"
  }
}
